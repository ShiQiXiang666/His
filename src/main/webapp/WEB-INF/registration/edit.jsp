<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
    <title>挂号-- -2015</title>
    <meta charset="UTF-8">
    <link rel="stylesheet" type="text/css" href="<%=request.getContextPath() %>/Css/bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="<%=request.getContextPath() %>/Css/bootstrap-responsive.css" />
    <link rel="stylesheet" type="text/css" href="<%=request.getContextPath() %>/Css/style.css" />
    <script type="text/javascript" src="<%=request.getContextPath() %>/Js/jquery.js"></script>
    <script type="text/javascript" src="<%=request.getContextPath() %>/Js/jquery.sorted.js"></script>
    <script type="text/javascript" src="<%=request.getContextPath() %>/Js/bootstrap.js"></script>
    <script type="text/javascript" src="<%=request.getContextPath() %>/Js/ckform.js"></script>
    <script type="text/javascript" src="<%=request.getContextPath() %>/Js/common.js"></script>
    <script type="text/javascript" src="<%=request.getContextPath() %>/Js/ckeditor/ckeditor.js"></script>
 

    <style type="text/css">
        body {
            padding-bottom: 40px;
        }
        .sidebar-nav {
            padding: 9px 0;
        }

        @media (max-width: 980px) {
            /* Enable use of floated navbar text */
            .navbar-text.pull-right {
                float: none;
                padding-left: 5px;
                padding-right: 5px;
            }
        }


    </style>
    <script type="text/javascript">
    $(function () {       
		$('#backid').click(function(){
				window.location.href="<%=request.getContextPath() %>/returnindex";
		 });
    });
    </script>
</head>
<body>
<form action="<%=request.getContextPath() %>/editReg" method="post" class="definewidth m20">
<table class="table table-bordered table-hover definewidth m10">
    <tr>
        <td width="10%" class="tableleft">姓名</td>
        <td><input type="text" name="name" value="${reg.name }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">证件类型</td>
        <td>
        <select name="certificateType" >
	        <option value="1" ${reg.certificateType=='1'?'selected':'' }>身份证</option>
	        <option value="2" ${reg.certificateType=='2'?'selected':'' }>护照</option>
	        <option value="3" ${reg.certificateType=='3'?'selected':'' }>军人证</option>
        </select>
        </td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">证件号</td>
        <td><input type="text" name="idNum" value="${reg.idNum }" /></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">挂号费</td>
        <td><input type="text" name="registrationFee" value="${reg.registrationFee }"/>元</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">社保号</td>
        <td><input type="text" name="socialSafeNum" value="${reg.socialSafeNum }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">联系电话</td>
        <td><input type="text" name="phone" value="${reg.phone }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">是否自费</td>
        <td><input type="radio" name="selfPaying" value="0" ${reg.selfPaying==0?'checked':'' }/>否&nbsp;&nbsp;&nbsp;
        <input type="radio" name="selfPaying" value="1" ${reg.selfPaying==1?'checked':'' }/>是</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">性别</td>
        <td><input type="radio" name="sex" value="0" ${reg.sex==0?'checked':'' }/>男&nbsp;&nbsp;&nbsp;
        <input type="radio" name="sex" value="1" ${reg.sex==1?'checked':'' }/>女</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">年龄</td>
        <td><input type="text" name="age" value="${reg.age }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">职业</td>
        <td><input type="text" name="career" value="${reg.career }"/></td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">初复诊</td>
        <td><input type="radio" name="earlyAppointment" value="初诊" ${reg.earlyAppointment=='初诊'?'checked':'' }/>初诊&nbsp;&nbsp;&nbsp;
        <input type="radio" name="earlyAppointment" value="复诊" ${reg.earlyAppointment=='复诊'?'checked':'' }/>复诊</td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">所挂科室</td>
        <td>
	        <select name="sectionId">
		        <option value="4" ${reg.sectionId==4?'selected':'' }>急诊科</option>
		        <option value="2" ${reg.sectionId==2?'selected':'' }>骨科</option>
		        <option value="1" ${reg.sectionId==1?'selected':'' }>血液科</option>
		        <option value="3" ${reg.sectionId==3?'selected':'' }>外科</option>
		        <option value="5" ${reg.sectionId==5?'selected':'' }>妇科</option>
	        </select>
	       </td>
    </tr>
    <tr>
        <td width="10%" class="tableleft">指定医生</td>
        <td>
        <select name="doctorId">
	         <option value="1" ${reg.doctorId==1?'selected':'' }>华佗</option>
	         <option value="2" ${reg.doctorId==2?'selected':'' }>扁鹊</option>
	         <option value="3" ${reg.doctorId==3?'selected':'' }>李时珍</option>
        </select>
        </td>
    </tr>
	<tr>
        <td width="10%" class="tableleft">备注</td>
        <td><textarea name="noted" >${reg.noted }</textarea></td>
	</tr>
    <tr>
        <td colspan="2">
			<center>
				<button type="submit" class="btn btn-primary" type="button">保存</button> &nbsp;&nbsp;
				<button type="button" class="btn btn-success" name="backid" id="backid">返回列表</button>
			</center>
		</td>
    </tr>
</table>
